

<header class='header'>
  <div>
    {{> logo}}
  </div>

  <nav class='header_nav '>
    <div class='header_nav_right'>
      {{> nav list=header}}
    </div>
  </nav>
</header>
   
<main class="main">
  {{#with info}}
    <div class="info">
      <h1 class="info-title">{{title}}</h1>
      <p class="info-text">{{description}}</p>
    </div>
  {{/with}}

  {{#with form}}
  <form class='form'>
    <div class='form-input-list'>
      {{#each inputs}}
        <input
          type='{{type}}'
          placeholder='{{placeholder}}'
          class='form-input'
          name='{{name}}'
          id='form-input-{{id}}'
        />
      {{/each}}
    </div>

    <div class='form-checkbox'>
      <input
        type='checkbox'
        name='{{checkbox.name}}'
        id='{{checkbox.name}}'
      />
      <label
        for='{{checkbox.name}}'
      >{{checkbox.label}}</label>
    </div>

    <button
      type='submit'
      class='form-button'
    >{{button}}</button>

    <div class="hr"></div>
   
    <p class='form-info'>{{info}}</p>
  </form>
  {{/with}}
</main>

<div class="upd">
  {{#with updates}}
  <h6 class="upd-title">{{title}}</h6>
  <div class="upd-list">
    {{#each list}}
    <div class="upd-item">
      <img class="upd-img" src="data:image/png;base64,">
      
      <div class="upd-info">
        <div class="upd-user">{{nickname}}</div>
        <div class="upd-description">{{description}}</div>
      </div>
    
    </div>
    {{/each}}
    {{#with show_all}}
      <button class="upd-btn" href="{{url}}">{{text}}</button>
    {{/with}}
  </div>
  {{/with}}
</div>

<div class="expirience">
  {{#with expirience}}
  <h2 class="expirience-title">{{title}}<span class="expirience-badge">{{badge}}</span></h2> 
  <div class="expirience-list">
    {{#each list}}
      <div class="expirience-item">
        <strong class="expirience-name">{{name}}</strong>
        <p class="expirience-details">{{details}}</p>
      </div>
    {{/each}}
  </div>
  
  {{/with}}
</div>


<footer class="footer">
  <div>
    {{> logo}}
    <span class='footer_copyright'>Copyright 2023-2024</span>
  </div>


  {{#each footer}}
    <div>
      <span class='footer_title'>{{title}}</span>
      {{> nav list=links isVertical=true}}
    </div>
  {{/each}}
</footer>
